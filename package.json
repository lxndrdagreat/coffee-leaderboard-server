{
  "name": "coffee-leaderboard-server",
  "version": "0.1.1",
  "description": "A coffee consumption leaderboard",
  "main": "lib/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node lib/server.js",
    "watch-server": "tsc -w --project src/tsconfig.json",
    "lint": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean-build": "shx rm -rf ./lib",
    "build": "npm run clean-build && npm run build-server",
    "build-server": "tsc --project ./tsconfig.json",
    "seed-database": "node lib/seed-data.js"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "author": "Dan Alexander",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.26",
    "pino-pretty": "^4.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@epandco/unthink-foundation": "^2.1.0",
    "@epandco/unthink-foundation-express": "^2.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.4"
  },
  "pre-commit": {
    "run": [
      "lint"
    ],
    "silent": true
  },
  "unthink": {
    "version": "3.0.7"
  },
  "contributors": [],
  "private": true
}
